{
  "name": "mobx-react-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for MobX + React project with JSX, ES6 compilation and hot code reloading",
  "scripts": {
    "start": "NODE_ENV=local node server.js",
    "lint": "eslint src",
    "webpack": "webpack --colors --progress --bail",
    "build": "NODE_ENV=production npm run webpack",
    "build:stage": "NODE_ENV=stage npm run webpack",
    "build:test": "NODE_ENV=test npm run webpack",
    "build:qa": "NODE_ENV=testing npm run webpack",
    "build:qa-manual": "NODE_ENV=qa-manual npm run webpack",
    "build:develop": "NODE_ENV=develop npm run webpack",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mobxjs/mobx-react-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "mobx",
    "starter-kit"
  ],
  "author": "Michel Weststrate <mweststrate@gmail.com> (http://github.com/mweststrate)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mobxjs/mobx/issues"
  },
  "homepage": "http://mobxjs.github.com/mobx",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "@kadira/storybook-addons": "^1.3.0",
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "html-webpack-plugin": "^2.28.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "aframe": "^0.5.0",
    "aframe-animation-component": "^3.2.5",
    "aframe-react": "^3.5.0",
    "aframe-text-geometry-component": "^0.5.1",
    "babel-polyfill": "^6.23.0",
    "express": "^4.15.2",
    "jss": "^6.3.0",
    "material-ui": "^0.17.0",
    "mobx": "^3.1.0",
    "mobx-react": "^4.1.0",
    "mobx-react-form": "^1.20.2",
    "moment": "^2.17.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-socket-io": "^0.2.4",
    "react-tap-event-plugin": "^2.0.1",
    "socket.io": "^1.7.3"
  }
}
